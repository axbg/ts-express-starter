{
  "name": "ts-express-starter",
  "version": "1.0.0",
  "repository": "https://github.com/axbg/ts-express-starter",
  "description": "A powerful express template based on TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint -c .eslintrc.js --ext .ts src/",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "dev": "tsc-watch --onSuccess \"node .\"",
    "test": "jest",
    "test-watch": "jest --watchAll",
    "coverage": "jest --collectCoverage",
    "migrate": "tsc && typeorm migration:generate -n"
  },
  "author": "axbg",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run prebuild && git add ."
    }
  },
  "dependencies": {
    "@types/client-sessions": "^0.8.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "1.17.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.19",
    "@types/passport": "^1.0.5",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth": "^1.0.41",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^2.2.0",
    "client-sessions": "^0.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.19.2",
    "express-session": "^1.17.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.8",
    "passport": "^0.4.1",
    "passport-github2": "^0.1.12",
    "passport-google-oauth": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "routing-controllers-openapi": "^3.1.0",
    "swagger-ui-express": "^4.1.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.29",
    "typeorm-typedi-extensions": "^0.2.3",
    "typescript": "^4.1.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.17.0",
    "eslint-plugin-jsdoc": "^30.7.13",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "husky": "^4.3.6",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.2.9"
  }
}
